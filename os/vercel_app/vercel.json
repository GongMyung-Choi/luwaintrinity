{
  "version": 2,
  "name": "luwain-app",
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "assets/**/*", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/signup", "dest": "/signup.html" },
    { "src": "/login", "dest": "/login.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
